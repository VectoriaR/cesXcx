.container {
  padding: 0;
  margin: 0;
}
.page-body {
  width: 100%;
}
image {
  width: 100%;
}
.title {
  margin: 10px 0 10px 0;
  font-size: 35px;
  font-weight: bold;
}
.btn-list {
  padding: 0 10px;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.btn-wrap {
  display: flex;
  width: 90%;
  align-items: center;
  justify-content: space-between;
  background: #f2f2f2;
  margin-bottom: 10px;
  padding: 15px 10px;
}
